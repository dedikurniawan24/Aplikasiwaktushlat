<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.dedi.aplikasitimeshalat">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/kubah"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/kubah"
            android:supportsRtl="true"
            android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <action android:name="android.intent.action.VIEW"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Activity_Jadwal"/>
        <activity android:name=".Activity_Halal"/>
        <activity android:theme="@style/AppTheme" android:name=".ActivityAsmaulHusna" />
        <activity android:theme="@style/AppTheme.NoActionBar" android:name=".ActivityAsmaulHusnaDetail" />
    </application>

</manifest>